{
  "name": "vue_fs_server",
  "version": "0.9.0",
  "description": "vue-fs backend",
  "main": "build/index.js",
  "scripts": {
    "db:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "db:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "db:down": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "db:up": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "start": "node build/.",
    "build": "tsc",
    "lint": "eslint --fix --ext .ts,.tsx .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts"
  },
  "author": "Derek Flynn",
  "license": "MIT",
  "dependencies": {
    "archiver": "^5.3.0",
    "class-transformer": "^0.4.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "extract-zip": "^2.0.1",
    "http-terminator": "^3.0.0",
    "mv": "^2.1.1",
    "path": "^0.12.7",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.0.2",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@types/archiver": "^5.1.0",
    "@types/cors": "^2.8.10",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/express-fileupload": "^1.1.6",
    "@types/mv": "^2.1.1",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "dotenv": "^9.0.2",
    "eslint": "^7.26.0",
    "eslint-plugin-vue": "^7.9.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  }
}
